{
  "src": [
    {
      "CloudStorm": [
        "./src/**/*.js"
      ]
    },
    {
      "index": "./README.md"
    }
  ],
  "dest": "./CloudStorm",
  "app": {
    "title": "CloudStorm",
    "base": "/CloudStorm",
    "entrance": "content:index",
    "routing": "query",
    "server": "github"
  },
  "jsdoc": {
    "access": "public",
    "plugins": [
      "plugins/markdown"
    ]
  },
  "markdown": {
    "gfm": true,
    "tables": true,
    "breaks": false,
    "pedantic": false,
    "sanitize": false,
    "smartLists": false,
    "smartypants": false,
    "tasks": false,
    "emoji": true
  },
  "template": {
    "options": {
      "navItems": [
        {
          "label": "Home",
          "href": "?content=index"
        },
        {
          "label": "Documentation",
          "href": "?api=CloudStorm#Client"
        }
      ]
    }
  }
}